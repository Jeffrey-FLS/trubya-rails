
<%= render 'layouts/header' %>

<div class="container">
  <div class="row">
    <div class="main offset-3 col-6 offset-3">

      <h1 style="text-align: center">List of Users</h1>


      <% @users.each do |user| %>
  <h3>
    <%= user.to_s %>
    <%= link_to "Show", user %>
    <%= link_to "Edit", edit_user_path(user) %>
    <%= link_to "Delete", user, method: :delete, data:
      { confirm: "Are you sure you want to delete this user?" } %>
  </h3>
<% end %>

<%= render 'welcome/back_admin_menu' %>

    </div>
    </div>
    </div>