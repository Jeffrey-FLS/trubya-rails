
<div class="main_-_form">

  <%= render 'layouts/flash' %>

  <%= form_for user, url: url do |f| %>
    <center>
      <%#= f.label :first_name %>
      <!--  <%#= f.text_field :first_name %><br />-->
      <%= f.text_field :first_name, placeholder: "First Name" %><br/>

      <%#= f.label :last_name %>
      <!--  <%#= f.text_field :last_name %><br />-->
      <%= f.text_field :last_name, placeholder: "Last Name" %><br/>

      <%#= f.label :username %>
      <!--  <%#= f.text_field :username %><br />-->
      <%= f.text_field :username, placeholder: "Username" %><br/>

      <%#= f.label :password %>
      <!--  <%#= f.password_field :password %><br />-->
      <%= f.password_field :password, placeholder: "Password" %><br/>

      <%#= f.label :password_confirmation %>
      <!--  <%#= f.password_field :password_confirmation %><br />-->
      <%= f.password_field :password_confirmation, placeholder: "Password Confirmation" %><br/>
      <%= f.submit %>
    </center>

  <% end %>
</div>